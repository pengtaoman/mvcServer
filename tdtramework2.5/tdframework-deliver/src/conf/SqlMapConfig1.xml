<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

  <settings
     cacheModelsEnabled="true"
     enhancementEnabled="true"
     lazyLoadingEnabled="true"
     maxRequests="32"
     maxSessions="10"
     maxTransactions="5"
     useStatementNamespaces="true" />
     
     
      <!-- 配置domain对象 -->
	   <typeAlias alias="oneModel" type="com.neusoft.tdframework.ibaguice.pot.domain.OneModel" />
	   <typeAlias alias="twoModel" type="com.neusoft.tdframework.ibaguice.pot.domain.TwoModel" />


      <!-- 配置xml映射文件 -->
	   <sqlMap resource="com/neusoft/tdframework/ibaguice/pot/domain/sqlmap/OneModel.xml" />
	   <sqlMap resource="com/neusoft/tdframework/ibaguice/pot/domain/sqlmap/TwoModel.xml" />
	   
</sqlMapConfig>
