<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<description>Home dataAccessContext</description>
	<bean id="homepageDAO" class="com.neusoft.tdframework.home.action.HomePageDAOImpl">
		<property name="dataSource">
			<ref bean="om_dataSource"/>
		</property>
	</bean>
	
	<bean id="portalpageDAO" class="com.neusoft.tdframework.home.action.PortalPageDAOImpl">
		<property name="dataSource">
			<ref bean="om_dataSource"/>
		</property>
	</bean>
	
	<bean id="omQueryDAO" class="com.neusoft.crm.channel.outInterface.om.dao.OmQueryDAOImpl">
		<property name="dataSource">
			<ref bean="om_dataSource"/>
		</property>
	</bean>
	
	<bean id="promptInfoDAO" class="com.neusoft.tdframework.message.dao.PromptInfoDAOImpl">
		<property name="dataSource">
			<ref bean="om_dataSource"/>
		</property>
	</bean>
	
	<bean id="schedulerMonitorDAO" class="com.neusoft.tdframework.quartz.dao.SchedulerMonitorDAOImpl">
		<property name="dataSource">
			<ref bean="om_dataSource"/>
		</property>
	</bean>
	
	<bean id="unlockDAO" class="com.neusoft.om.dao.unlock.UnlockDAOImpl">
		<property name="dataSource">
			<ref bean="om_dataSource"/>
		</property>
	</bean>
	<bean id="loginFailLogDAO" class="com.neusoft.om.dao.loginFail.LoginFailLogDAOImpl">
		<property name="dataSource">
			<ref bean="om_dataSource"/>
		</property>
	</bean>
	
	<bean id="loginActionDAO" class="com.neusoft.tdframework.authorization.action.LoginActionDaoImpl">
		<property name="dataSource">
			<ref bean="om_dataSource"/>
		</property>
	</bean>

	<bean id="om_dataSource" class="org.springframework.jndi.JndiObjectFactoryBean">
	<property name="jndiName">
		<value>java:comp/env/jdbc/crm</value>
	</property>
	</bean>
		
</beans>
