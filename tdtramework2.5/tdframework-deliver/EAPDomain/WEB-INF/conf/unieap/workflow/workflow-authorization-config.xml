<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<!-- procdef authorization -->
		<form-bean name="procauthlistForm"
			type="com.neusoft.uniflow.web.authorization.uniresource.forms.ProcDefListForm" />
		<form-bean name="procSVGViewForm"
			type="com.neusoft.uniflow.web.authorization.uniresource.forms.ProcessSVGViewForm" />
	</form-beans>
	<action-mappings>
		<!--resouce authorization about form and so on. shangzf@2007-3-6-->
		<action name="procauthlistForm"
			type="com.neusoft.uniflow.web.authorization.uniresource.actions.ProcAuthAction"
			scope="request" path="/procauthlist">
			<forward name="success"
				path="/unieap/pages/workflow/authorization/uniresource/procdeflist.jsp" />
		</action>
		<action
			type="com.neusoft.uniflow.web.authorization.uniresource.actions.ParticipantsAction" input="/unieap/pages/workflow/authorization/uniresource/partitree.jsp" scope="request" path="/authpartitree">
			<forward name="success"
				path="/unieap/pages/workflow/authorization/uniresource/partitree.jsp" />
		</action>
	    <action type="com.neusoft.uniflow.web.authorization.uniresource.actions.CopyParticipantsAction" input="/unieap/pages/workflow/authorization/uniresource/nodiparti.jsp" scope="request" path="/nodeparticipants">
            <forward name="success" path="/unieap/pages/workflow/authorization/uniresource/nodeparti.jsp" />
        </action>
		<action forward="/unieap/pages/workflow/authorization/uniresource/formauthWindow.jsp" path="/formauthWindow" />
		<action forward="/unieap/pages/workflow/authorization/uniresource/gisauthWindow.jsp" path="/gisauthWindow" />
		<action forward="/unieap/pages/workflow/authorization/uniresource/docmauthWindow.jsp" path="/docauthWindow" />
		<!--  流程相关授权部分 -->
		<action type="com.neusoft.uniflow.web.management.purview.Actipurview" scope="request" parameter="method" path="/uniflowActipurview">
			<forward name="begin"
					path="/unieap/pages/workflow/authorization/uniresource/resourcetree/index.jsp" />
			<forward name="list" path="/unieap/pages/workflow/authorization/uniresource/resourcetree/list.jsp" />
	        <forward name="tree" path="/unieap/pages/workflow/authorization/uniresource/resourcetree/menutree.jsp" />
		</action>
		<action forward="/unieap/pages/workflow/authorization/uniresource/resourcetree/notepanel.jsp"  path="/uniflownotepanelAction" />	    
	    <action forward="/unieap/pages/workflow/authorization/uniresource/resourcetree/buttonpanel.jsp" path="/uniflowbuttonpanelAction" />
	    <!-- 集成文件 -->
	    <action forward="/unieap/pages/workflow/authorization/purview/skip.jsp" path="/skipworkflow" />
	    <action name="procSVGViewForm"
			type="com.neusoft.uniflow.web.authorization.uniresource.actions.ProcessSVGViewAction"
			scope="request" path="/uniresourceAuthor">
			<forward name="newversion"
				path="/unieap/pages/workflow/authorization/uniresource/processview.jsp" />
			<forward name="oldversion"
				path="/unieap/pages/workflow/authorization/uniresource/oldprocessview.jsp" />
			<forward name="copynewversion"
				path="/unieap/pages/workflow/authorization/uniresource/copyprocessview.jsp" />
			<forward name="copyoldversion"
				path="/unieap/pages/workflow/authorization/uniresource/copyoldprocessview.jsp" />
		</action>
	</action-mappings>
</struts-config>
