<?xml version="1.0" encoding="UTF-8"?>
<views xmlns:bsdesign="http://workflow.neusoft.com/bsdesign/">
<applicationControlBar>
     <newFileBtn visible = "true">
        <new_process visible="true"/>
        <new_process_by_mode visible="true"/>
     </newFileBtn>
     <tempSaveBtn visible = "true"/>
     <saveBtn visible = "true"/>
     <importBtn visible = "true">
        <import_process_from_db visible="true"/>
        <import_process_file visible="true"/>
     </importBtn>
     <deployBtn visible = "true">
        <export_file visible="true"/>
        <export_image visible="true"/>
     </deployBtn>
     <copyBtn visible = "true"/>
     <pasteBtn visible = "true"/>
     <delBtn visible = "true"/>
     <zoomNoBtn visible = "true"/>
     <zoomOutBtn visible = "true"/>
     <zoomInBtn visible = "true"/>
     <alignLeftBtn visible = "true"/>
     <alignCenterBtn visible = "true"/>
     <alignRightBtn visible = "true"/>
     <alignTopBtn visible = "true"/>
     <alignMiddleBtn visible = "true"/>
     <alignBottomBtn visible = "true"/>
     <printBtn visible = "true"/>
</applicationControlBar>

<toolPanel>
     <selectBtn visible = "true"/>
     <transitionBtn visible = "true"/>
     <startNodeBtn visible = "true"/>
     <endNodeBtn visible = "true"/>
     <choiceNodeBtn visible = "true"/>
     <manualNodeBtn visible = "true" initParticipant="creator"/>
     <autoNodeBtn visible = "true"/>
     <subprocNodeBtn visible = "true"/>
     <parallelNodeBtn visible = "true"/>
     <nodeTempletBtn visible = "true"/>
     <variableBtn visible = "true"/>
</toolPanel>

<validate>
     <mobileNetStrategy enable="false"/>
</validate>

<operationPolicy enable="false">
</operationPolicy>

<preferences>
     <category enable="true"/>
     <tooltip>
          <manualnodeDetail enable="true"/>
     </tooltip>
</preferences>

</views>
