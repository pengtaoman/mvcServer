<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<!-- ========== Form Bean Definitions =================================== -->
	<form-beans>
		<!-- workitem list -->
		<form-bean name="personWorkListForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.mywork.forms.PersonWorkListForm" />
		<form-bean name="roleWorkListForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.rolework.forms.RoleWorkListForm" />
		<form-bean name="agentWorkListForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.agentwork.forms.AgentWorkListForm" />
		<form-bean name="allWorkListForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.allwork.forms.AllWorkListForm" />
		<form-bean name="overtimeWorkListForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.overtimework.forms.OverTimeWorkListForm" />
		<!--  workitem operation -->
		<form-bean name="adhocForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.forms.AdhocForm" />
		<form-bean name="sendtoForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.forms.SendtoForm" />
		<form-bean name="nexttoForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.forms.NexttoForm" />
		<form-bean name="nextActForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.forms.NextActForm" />
		<form-bean name="updatePartiActionForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.forms.UpdatePartiActionForm" />
		<form-bean name="reassignForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.forms.ReassignForm" />
		<form-bean name="openworkitemForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.forms.OpenWorkitemForm" />
		<form-bean name="rollBackForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.forms.RollBackForm" />
		<!--  procdef list -->
		<form-bean name="procdeflistForm"
			type="com.neusoft.uniflow.web.standard.procdefmgt.procdeflist.forms.ProcDefListForm" />
		<!-- create and start process -->
		<form-bean name="createProcInstForm"
			type="com.neusoft.uniflow.web.standard.procdefmgt.creatandstart.forms.CreateProcInstForm" />
		<form-bean name="creatroleForm"
			type="com.neusoft.uniflow.web.standard.procdefmgt.creatandstart.forms.CreatRoleForm" />
		<!-- procinst list -->
		<form-bean name="procinstForm"
			type="com.neusoft.uniflow.web.standard.procinstmgt.procinstlist.forms.ProcInstForm" />
		<form-bean name="rdForm"
			type="com.neusoft.uniflow.web.standard.procinstmgt.procinstlist.forms.RDForm" />
		<form-bean name="createNewVersionForm" 
		type="com.neusoft.uniflow.web.standard.procinstmgt.procinstlist.forms.CreateNewVersionForm"></form-bean>
		<form-bean name="sttransitionForm"
		type="com.neusoft.uniflow.web.standard.workitemmgt.operation.forms.TransitionForm"></form-bean>
	</form-beans>
	<action-mappings>
		<!-- my work -->
		<action name="personWorkListForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.mywork.actions.PersonWorkListAction"
			scope="request" path="/mywork">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/list/mywork.jsp" />
		</action>
		<!-- role work -->
		<action name="roleWorkListForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.rolework.actions.RoleWorkListAction"
			scope="request" path="/rolework">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/list/rolework.jsp" />
		</action>
		<!-- agent work -->
		<action name="agentWorkListForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.agentwork.actions.AgentWorkListAction"
			scope="request" path="/agentwork">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/list/agentwork.jsp" />
		</action>
		<!-- all work -->
		<action name="allWorkListForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.allwork.actions.AllWorkListAction"
			scope="request" path="/allwork">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/list/allwork.jsp" />
		</action>
		<!-- overtime work -->
		<action name="overtimeWorkListForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.overtimework.actions.OverTimeWorkListAction"
			scope="request" path="/overtimework">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/list/overtimework.jsp" />
		</action>
		<!-- workitem operation -->
		<action name="adhocForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.actions.AdhocAction"
			scope="request" path="/adhoc">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/operation/adhoc.jsp" />
		</action>
		<action name="sendtoForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.actions.SendtoAction"
			scope="request" path="/sendto">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/operation/sendto.jsp" />
		</action>
		<action name="nexttoForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.actions.NexttoAction"
			scope="request" path="/nextto">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/operation/nextto.jsp" />
		</action>
		<action name="nextActForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.actions.NextActAction"
			scope="request" path="/nextAct">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/operation/nextactdo.jsp" />
		</action>
		<action name="updatePartiActionForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.actions.UpdatePartiAction"
			scope="request" path="/updateParti">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/operation/updateparti.jsp" />
		</action>
		<action name="reassignForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.actions.ReassignAction"
			scope="request" path="/reassign">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/operation/reassign.jsp" />
		</action>
		<action name="openworkitemForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.actions.OpenWorkitemAction"
			scope="request" path="/openworkitem">
			<forward name="taskhandler"
				path="/unieap/pages/workflow/standard/workitem/operation/taskhandler.jsp" />
			<forward name="taskviewer"
				path="/unieap/pages/workflow/standard/workitem/operation/taskviewer.jsp" />
		</action>
		<action name="openworkitemForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.actions.TaskHandlerAction"
			scope="request" path="/taskhandler">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/operation/handler.jsp" />
		</action>
		<action name="rollBackForm"
			type="com.neusoft.uniflow.web.standard.workitemmgt.operation.actions.RollBackAction"
			scope="request" path="/rollback">
			<forward name="success"
				path="/unieap/pages/workflow/standard/workitem/operation/rollback.jsp" />
		</action>
		<!-- procdef list -->
		<action name="procdeflistForm"
			type="com.neusoft.uniflow.web.standard.procdefmgt.procdeflist.actions.ProcDefListAction"
			scope="request" path="/procdeflist">
			<forward name="success"
				path="/unieap/pages/workflow/standard/procdef/list/procdeflist.jsp" />
		</action>
		<!--  create and start process -->
		<action name="createProcInstForm"
			type="com.neusoft.uniflow.web.standard.procdefmgt.creatandstart.actions.CreateProcInstAction"
			scope="request" path="/createProcInst">
			<forward name="success"
				path="/unieap/pages/workflow/standard/procdef/operation/createprocinst.jsp" />
		</action>
		<action name="createProcInstForm"
			type="com.neusoft.uniflow.web.standard.procdefmgt.creatandstart.actions.UpdateProcInstAction"
			scope="request" path="/updateProcInst">
			<forward name="success"
				path="/unieap/pages/workflow/standard/procdef/operation/createprocinst.jsp" />
		</action>
		<action name="creatroleForm"
			type="com.neusoft.uniflow.web.standard.procdefmgt.creatandstart.actions.CreatRoleAction"
			scope="request" path="/creatrole">
			<forward name="success"
				path="/unieap/pages/workflow/standard/procdef/operation/creat_role.jsp" />
		</action>
		<action name="creatroleForm"
			type="com.neusoft.uniflow.web.standard.procdefmgt.creatandstart.actions.UpdateCreatRoleAction"
			scope="request" path="/updatecreatrole">
			<forward name="success"
				path="/unieap/pages/workflow/standard/procdef/operation/creat_role.jsp" />
		</action>
		<!--  procinst list -->
		<action name="procinstForm"
			type="com.neusoft.uniflow.web.standard.procinstmgt.procinstlist.actions.ProcInstAction"
			scope="request" path="/procinst">
			<forward name="success"
				path="/unieap/pages/workflow/standard/procinst/list/procinst.jsp" />
		</action>
		<action name="rdForm"
			type="com.neusoft.uniflow.web.standard.procinstmgt.procinstlist.actions.RDAction"
			scope="request" path="/rd">
			<forward name="success"
				path="/unieap/pages/workflow/standard/procinst/list/rd.jsp" />
		</action>
		<action name="rdForm"
			type="com.neusoft.uniflow.web.standard.procinstmgt.procinstlist.actions.UpdateRDAction"
			scope="request" path="/updateRD">
			<forward name="success"
				path="/unieap/pages/workflow/standard/procinst/list/rd.jsp" />
		</action>
		<action name="createNewVersionForm" 
		type="com.neusoft.uniflow.web.standard.procinstmgt.procinstlist.actions.CreateNewVersionAction" 
		path="/createversion">
		<forward name="success" path="/unieap/pages/workflow/standard/procinst/operation/createnewversion.jsp"></forward>
		</action>
		<action name="sttransitionForm" 
		type="com.neusoft.uniflow.web.standard.workitemmgt.operation.actions.TransitionAction" path="/tranList">
		<forward name="success" path="/unieap/pages/workflow/standard/workitem/operation/transationList.jsp"></forward>
		</action>
	</action-mappings>
</struts-config>
