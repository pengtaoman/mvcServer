<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<!-- procdef monitor -->
		<form-bean name="procmonitorlistForm"
			type="com.neusoft.uniflow.web.monitor.procdef.forms.ProcDefListForm" />
		<form-bean name="procInstListForm"
			type="com.neusoft.uniflow.web.monitor.procdef.forms.ProcInstListForm" />
		<form-bean name="workItemListForm"
			type="com.neusoft.uniflow.web.monitor.procdef.forms.WorkItemListForm" />
		<form-bean name="procInstDetailForm"
			type="com.neusoft.uniflow.web.monitor.procdef.forms.ProcInstDetailForm" />
		<form-bean name="curActDetailForm"
			type="com.neusoft.uniflow.web.monitor.procdef.forms.CurActDetailForm" />
		<form-bean name="applicationlistForm"
			type="com.neusoft.uniflow.web.monitor.procdef.forms.ApplicationListForm" />
		<form-bean name="applicationdetailForm"
			type="com.neusoft.uniflow.web.monitor.procdef.forms.ApplicationDetailForm" />
		<form-bean name="svgMonitorForm"
			type="com.neusoft.uniflow.web.monitor.procdef.forms.SVGMonitorForm" />
		<!-- process instance monitor -->
		<form-bean name="procDetailForm"
			type="com.neusoft.uniflow.web.monitor.procinst.forms.DetailForm" />
		<form-bean name="procMonitorForm"
			type="com.neusoft.uniflow.web.monitor.procinst.forms.SVGMonitorForm" />
		<!-- parallel -->	
	    <form-bean name="parallelBranchForm"
			type="com.neusoft.uniflow.web.monitor.procinst.forms.ParallelBranchForm" />
		<form-bean name="oldParallelBranchForm"
			type="com.neusoft.uniflow.web.monitor.procinst.forms.OldParallelBranchForm" />
		<!-- rollback -->
		<form-bean name="monitorrollBackForm"
			type="com.neusoft.uniflow.web.monitor.procinst.forms.RollBackForm" />
		<form-bean name="parallelform" 
			type="com.neusoft.uniflow.web.monitor.procinst.forms.ParallelDetailForm" />
	</form-beans>
	<action-mappings>
		<!-- procdef monitor list -->
		<action name="procmonitorlistForm"
			type="com.neusoft.uniflow.web.monitor.procdef.actions.ProcDefListAction"
			scope="request" path="/procmonitorlist">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procdef/procdeflist.jsp" />
		</action>
		<action name="procInstListForm"
			type="com.neusoft.uniflow.web.monitor.procdef.actions.ProcInstListAction"
			scope="request" path="/monitorproc">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procdef/monitorproc.jsp" />
		</action>
		<action name="workItemListForm"
			type="com.neusoft.uniflow.web.monitor.procdef.actions.WorkItemListAction"
			scope="request" path="/monitorworkitem">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procdef/monitorworkitem.jsp" />
		</action>
		<action name="workItemListForm"
			type="com.neusoft.uniflow.web.monitor.procdef.actions.WorkItemDetailAction"
			scope="request" path="/workitemdetail">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procdef/workitemdetail.jsp" />
		</action>
		<action name="procInstDetailForm"
			type="com.neusoft.uniflow.web.monitor.procdef.actions.ProcInstDetailAction"
			scope="request" path="/procinstdetail">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procdef/procinstdetail.jsp" />
		</action>
		<action name="curActDetailForm"
			type="com.neusoft.uniflow.web.monitor.procdef.actions.CurActDetailAction"
			scope="request" path="/curactdetail">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procdef/curactdetail.jsp" />
		</action>

		<action name="applicationlistForm"
			type="com.neusoft.uniflow.web.monitor.procdef.actions.ApplicationListAction"
			scope="request" path="/applicationlist">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procdef/applicationlist.jsp" />
		</action>
		<action name="applicationdetailForm"
			type="com.neusoft.uniflow.web.monitor.procdef.actions.ApplicationDetailAction"
			scope="request" path="/applicationdetail">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procdef/applicationdetail.jsp" />
		</action>
		<action name="svgMonitorForm"
			type="com.neusoft.uniflow.web.monitor.procdef.actions.SVGMonitorAction"
			scope="request" path="/svgmonitor">
			<forward name="newversion"
				path="/unieap/pages/workflow/monitor/procdef/svgmonitor.jsp" />
			<forward name="oldversion"
				path="/unieap/pages/workflow/monitor/procdef/oldsvgmonitor.jsp" />
		</action>
		<!-- process instance monitor -->
		<action name="procDetailForm"
			type="com.neusoft.uniflow.web.monitor.procinst.actions.DetailAction"
			scope="request"
			path="/unieap/pages/workflow/monitor/procinst/procdetail">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procinst/procdetail.jsp" />
		</action>
		<action name="procMonitorForm"
			type="com.neusoft.uniflow.web.monitor.procinst.actions.SVGMonitorAction"
			scope="request" path="/procmonitor">
			<forward name="newversion"
				path="/unieap/pages/workflow/monitor/procinst/procmonitor.jsp" />
			<forward name="oldversion"
				path="/unieap/pages/workflow/monitor/procinst/oldprocmonitor.jsp" />
		</action>
		<action
			type="com.neusoft.uniflow.web.monitor.procinst.actions.ProcInstDetailAction"
			scope="request" path="/pdetail">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procinst/procinstdetail.jsp" />
		</action>
		<action
			type="com.neusoft.uniflow.web.monitor.procinst.actions.ActDefDetailAction"
			scope="request" path="/addetail">
			<forward name="manul" path="/unieap/pages/workflow/monitor/procinst/actdef_manul.jsp" />
			<forward name="auto" path="/unieap/pages/workflow/monitor/procinst/actdef_auto.jsp" />
			<forward name="event" path="/unieap/pages/workflow/monitor/procinst/actdef_event.jsp" />
			<forward name="subproc" path="/unieap/pages/workflow/monitor/procinst/actdef_subproc.jsp" />
		</action>
		<action
			type="com.neusoft.uniflow.web.monitor.procinst.actions.ActInstDetailAction"
			scope="request" path="/aidetail">
			<forward name="manul" path="/unieap/pages/workflow/monitor/procinst/actinst_manul.jsp" />
			<forward name="auto" path="/unieap/pages/workflow/monitor/procinst/actinst_auto.jsp" />
			<forward name="event" path="/unieap/pages/workflow/monitor/procinst/actinst_event.jsp" />
		</action>
		<action
			type="com.neusoft.uniflow.web.monitor.procinst.actions.RelDateDetailAction"
			scope="request" path="/rddetail">
			<forward name="procinst" path="/unieap/pages/workflow/monitor/procinst/rd_procinst.jsp" />
			<forward name="procdef" path="/unieap/pages/workflow/monitor/procinst/rd_procdef.jsp" />
			<forward name="manul" path="/unieap/pages/workflow/monitor/procinst/rd_manul.jsp" />
			<forward name="auto" path="/unieap/pages/workflow/monitor/procinst/rd_auto.jsp" />
			<forward name="event" path="/unieap/pages/workflow/monitor/procinst/rd_event.jsp" />
		</action>
		<action
			type="com.neusoft.uniflow.web.monitor.procinst.actions.WorkItemListAction"
			scope="request" path="/widetail">
			<forward name="success" path="/unieap/pages/workflow/monitor/procinst/wilistdetail.jsp" />
		</action>
		<action
			type="com.neusoft.uniflow.web.monitor.procinst.actions.ApplicationDetailAction"
			scope="request" path="/apdetail">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procinst/applicationdetail.jsp" />
		</action>
		<action
			type="com.neusoft.uniflow.web.monitor.procinst.actions.EventListAction"
			scope="request" path="/eventlist">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procinst/eventlist.jsp" />
		</action>
		<!-- parallel -->			
		<action name="parallelBranchForm"
			type="com.neusoft.uniflow.web.monitor.procinst.actions.ParallelBranchAction"
			scope="request" path="/parallel">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procinst/parallelbranch.jsp" />
		</action>
		<action name="oldParallelBranchForm"
			type="com.neusoft.uniflow.web.monitor.procinst.actions.OldParallelBranchAction"
			scope="request" path="/oldparallel">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procinst/oldparallelbranch.jsp" />
		</action>
		<!-- rollback -->	
		<action name="monitorrollBackForm"
			type="com.neusoft.uniflow.web.monitor.procinst.actions.RollBackAction"
			scope="request" path="/monitorrollback">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procinst/rollback.jsp" />
		</action>
		<action type="com.neusoft.uniflow.web.monitor.procinst.actions.ParallelDetailAction" path="/paralleldetail" name="parallelform" scope="request">
			<forward name="success"
				path="/unieap/pages/workflow/monitor/procinst/paralleldetail.jsp" />
		</action>
		<action path="/blank" forward="/unieap/pages/workflow/monitor/procinst/blank.jsp"></action>
	</action-mappings>
</struts-config>
