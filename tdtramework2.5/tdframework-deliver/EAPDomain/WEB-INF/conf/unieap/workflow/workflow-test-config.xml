<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<!-- ========== Form Bean Definitions =================================== -->
	<form-beans>
		<form-bean name="psWorkListForm"
			type="com.neusoft.uniflow.web.test.forms.PersonWorkListForm" />
		<form-bean name="startProcForm"
			type="com.neusoft.uniflow.web.test.forms.StartProcForm" />
		<form-bean name="workHandlerForm"
			type="com.neusoft.uniflow.web.test.forms.WorkHandlerForm" />
	</form-beans>
	<action-mappings>
		<action name="psWorkListForm"
			type="com.neusoft.uniflow.web.test.actions.PersonWorkListAction"
			scope="request" path="/pswork">
			<forward name="success"
				path="/unieap/pages/workflow/test/mybizwork.jsp" />
		</action>
		<action name="startProcForm"
			type="com.neusoft.uniflow.web.test.actions.StartProcAction"
			scope="request" path="/startproc">
			<forward name="success"
				path="/unieap/pages/workflow/test/startproc.jsp" />
		</action>
		<action name="workHandlerForm"
			type="com.neusoft.uniflow.web.test.actions.WorkHandlerAction"
			scope="request" path="/workhandler">
			<forward name="success"
				path="/unieap/pages/workflow/test/workhandler.jsp" />
		</action>
	</action-mappings>
</struts-config>
