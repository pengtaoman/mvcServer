<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<!-- ========== Form Bean Definitions =================================== -->
	<form-beans>
		<form-bean name="logonForm"
			type="com.neusoft.uniflow.web.common.login.forms.LogonForm" />
		<form-bean name="prefForm"
			type="com.neusoft.uniflow.web.common.preference.forms.PreferenceForm" />
		<form-bean name="agentInfoForm"
			type="com.neusoft.uniflow.web.common.agent.forms.AgentInfoForm" />
		<form-bean name="customForm"
			type="com.neusoft.uniflow.web.common.custom.forms.CustomForm" />
		<!-- org tree -->
		<form-bean name="oneActorSelForm"
			type="com.neusoft.uniflow.web.common.trees.forms.OneActorSelForm" />
		
		<!-- notify -->
		<form-bean name="notifyForm"
			type="com.neusoft.uniflow.web.common.notify.forms.NotifyForm" />
		<form-bean name="notifyInfoForm"
			type="com.neusoft.uniflow.web.common.notify.forms.NotifyInfoForm" />
		<!-- agentlist -->
		<form-bean name="agentForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.agentlist.forms.AgentListForm" />

		<form-bean name="assigneeWorkListForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.agentlist.forms.AssigneeWorkListForm" />

		<form-bean name="assigneeBizWorkListForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.agentlist.forms.AssigneeBizWorkListForm" />

		<!-- create agent -->
		<form-bean name="createAgntForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.create.forms.CreateAgentForm" />
		<form-bean name="modifyAgntForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.create.forms.ModifyAgentForm" />
		<form-bean name="myagentCategoryForm"
		type="com.neusoft.uniflow.web.common.myagentmgt.create.forms.AgentCategoryForm"/>
		<!-- updown -->
		<form-bean name="fileUploadsForm"
			type="com.neusoft.uniflow.web.common.updown.forms.FileUploadsForm" />
		<form-bean name="fileDownloaForm"
			type="com.neusoft.uniflow.web.common.updown.forms.FileDownloadsForm" />
		<!-- add by hushiyuan 2008.03.07 -->
		<form-bean name="workdaycategoryListForm" type="com.neusoft.uniflow.web.common.workdayorgtree.forms.WorkDayOrgTreeForm"/>
		<!-- end add by hushiyuan -->
	</form-beans>
	<!-- ========== Global Forward Definitions ============================== -->
	<global-forwards>
		<forward name="logoff" path="/logout.do" />
		<forward name="logon" path="/unieap/pages/workflow/welcome.jsp" />
		<forward name="error"
			path="/unieap/pages/workflow/common/error.jsp" />
	</global-forwards>
	<!-- ========== Action Mapping Definitions ============================== -->
	<action-mappings>
		<!-- logon -->
		<action name="logonForm"
			type="com.neusoft.uniflow.web.common.login.actions.LogonAction"
			validate="false" input="/unieap/pages/workflow/welcome.jsp"
			scope="request" path="/logon">
			<forward name="FW2009_Main"
				path="/unieap/pages/workflow/common/workFlowFW.jsp" />
			<forward name="main"
				path="/unieap/pages/workflow/common/frameMain.jsp" />
			<forward name="relogon"
				path="/unieap/pages/workflow/common/relogon.jsp" />
			<forward name="test"
				path="/pswork.do?categoryview=biztest" />
		</action>
		<!-- For Common Style Start -->
		<action
			type="com.neusoft.uniflow.web.common.login.actions.ListTreeAction"
			scope="request" path="/listtree">
			<forward name="success"
				path="/unieap/pages/workflow/common/listtree.jsp" />
		</action>
		<!-- For Common Style End -->
		
		<!-- For Style 2009 Start -->
		<action
			type="com.neusoft.uniflow.web.common.login.actions.ListTreeAction2009"
			scope="request" path="/listtree2009">
			<forward name="success"
				path="/unieap/pages/workflow/common/listtree.jsp" />
			<forward name="Style2009Menu"
				path="/unieap/pages/workflow/menu/Style2009MenuTemplate.jsp" />
		</action>
		<!-- For Style 2009 End -->
		<action
			type="com.neusoft.uniflow.web.common.login.actions.TitleAction"
			scope="request" path="/title">
			<forward name="success"
				path="/unieap/pages/workflow/common/title.jsp" />
		</action>
		<!-- logout -->
		<action
			type="com.neusoft.uniflow.web.common.login.actions.LogoutAction"
			scope="request" path="/logout">
			<forward name="success"
				path="/unieap/pages/workflow/common/logout.jsp" />
		</action>
		<!-- agent -->
		<action name="agentInfoForm"
			type="com.neusoft.uniflow.web.common.agent.actions.AgentInfoAction"
			scope="request" path="/agentinfo">
			<forward name="success"
				path="/unieap/pages/workflow/common/agentinfo.jsp" />
		</action>
		<action name="assigneeWorkListForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.agentlist.actions.AssigneeWorkListAction"
			scope="request" path="/assigneework">
			<forward name="success"
				path="/unieap/pages/workflow/common/myagentmgt/assigneework.jsp" />
		</action>
		<action name="assigneeBizWorkListForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.agentlist.actions.AssigneeBizWorkListAction"
			scope="request" path="/assigneebizwork">
			<forward name="success"
				path="/unieap/pages/workflow/common/myagentmgt/assigneebizwork.jsp" />
		</action>
		<action name="agentForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.agentlist.actions.AgentListAction"
			scope="request" path="/agentlist_mine">
			<forward name="success"
				path="/unieap/pages/workflow/common/myagentmgt/agentlist.jsp" />
		</action>
		<action name="createAgntForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.create.actions.CreateAgentAction"
			scope="request" path="/agentcreate_mine">
			<forward name="success"
				path="/unieap/pages/workflow/common/myagentmgt/createagent.jsp" />
		</action>
		<action name="modifyAgntForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.create.actions.ModifyAgentAction"
			scope="request" path="/agentmodify_mine">
			<forward name="success"
				path="/unieap/pages/workflow/common/myagentmgt/modifyagent.jsp" />
		</action>
			<action name="myagentCategoryForm"
			type="com.neusoft.uniflow.web.common.myagentmgt.create.actions.AgentCategoryAction"
			scope="request" path="/myagentcptree">
			<forward name="success"
				path="/unieap/pages/workflow/common/myagentmgt/categorytree.jsp" />
		</action>
		<!-- custom -->
		<action name="customForm"
			type="com.neusoft.uniflow.web.common.custom.actions.CustomAction"
			scope="request" path="/custom">
			<forward name="success"
				path="/unieap/pages/workflow/common/custom.jsp" />
		</action>
		<action name="customForm"
			type="com.neusoft.uniflow.web.common.custom.actions.ModifyCustAction"
			scope="request" path="/modifycust">
			<forward name="success"
				path="/unieap/pages/workflow/common/custom.jsp" />
		</action>
		<action name="customForm"
			type="com.neusoft.uniflow.web.common.custom.actions.DefaultCustAction"
			scope="request" path="/defaultcust">
			<forward name="success"
				path="/unieap/pages/workflow/common/custom.jsp" />
		</action>
		<!-- workitem application display -->
		<action type="org.apache.struts.actions.ForwardAction"
			parameter="/unieap/pages/workflow/common/display.jsp" scope="request"
			path="/display" />
		<!-- org  tree -->
		<action name="oneActorSelForm"
			type="com.neusoft.uniflow.web.common.trees.actions.OneActorSelAction"
			scope="request" path="/oneactorsel">
			<forward name="success"
				path="/unieap/pages/workflow/common/oneactorsel.jsp" />
		</action>
		<!-- application  tree -->

		<action 
			type="com.neusoft.uniflow.web.common.trees.actions.OneApplicationSelAction"
			scope="request" path="/oneappsel">
			<forward name="success"
				path="/unieap/pages/workflow/common/apptree/oneapplicationsel.jsp" />
			<forward name="blank"
				path="/unieap/pages/workflow/common/apptree/blank.jsp" />
		</action>
	   <!-- 新增加的多Form办理页面 -->
		<action 
			type="com.neusoft.uniflow.web.common.trees.actions.MulApplicationSelAction"
			scope="request" path="/mulappsel">
			<forward name="success"
				path="/unieap/pages/workflow/common/apptree/mulapplicationsel.jsp" />
			<forward name="blank"
				path="/unieap/pages/workflow/common/apptree/blank.jsp" />
		</action>
		<!--preference-->
		<action name="prefForm"
			type="com.neusoft.uniflow.web.common.preference.actions.PreferenceAction"
			validate="false" input="success" scope="request" path="/pref">
			<forward name="success"
				path="/unieap/pages/workflow/common/preference/general.jsp" />
		</action>
		<action name="prefForm"
			type="com.neusoft.uniflow.web.common.preference.actions.GeneralPrefAction"
			validate="true"
			input="/unieap/pages/workflow/common/preference/general.jsp"
			scope="request" path="/generalpref">
			<forward name="password"
				path="/unieap/pages/workflow/common/preference/changepwd.jsp" />
			<forward name="OK"
				path="/unieap/pages/workflow/common/preference/general.jsp" />
		</action>
		<action name="prefForm"
			type="com.neusoft.uniflow.web.common.preference.actions.PwdPrefAction"
			validate="true"
			input="/unieap/pages/workflow/common/preference/changepwd.jsp"
			scope="request" path="/pwdpref">
			<forward name="general"
				path="/unieap/pages/workflow/common/preference/general.jsp" />
			<forward name="OK"
				path="/unieap/pages/workflow/common/preference/changepwd.jsp" />
		</action>
		<!--notify-->
		<action name="notifyForm"
			type="com.neusoft.uniflow.web.common.notify.actions.NotifyPrefAction"
			validate="true"
			input="/unieap/pages/workflow/common/notify/notify.jsp"
			scope="request" path="/notifypref">
			<forward name="OK"
				path="/unieap/pages/workflow/common/notify/notify.jsp" />
			<forward name="notify"
				path="/unieap/pages/workflow/common/notify/notify.jsp" />
		</action>
		<action name="notifyInfoForm"
			type="com.neusoft.uniflow.web.common.notify.actions.NotifyInfoAction"
			scope="request" path="/notifyinfo">
			<forward name="success"
				path="/unieap/pages/workflow/common/notify/notifyInfo.jsp" />
		</action>
		<!-- updown -->
		<action path="/fileUploads" name="fileUploadsForm"
			type="com.neusoft.uniflow.web.common.updown.actions.FileUploadsAction"
			validate="true" scope="request">
			<forward name="haveResult"
				path="/unieap/pages/workflow/common/updown/uploadsok.jsp" />
			<forward name="failure"
				path="/unieap/pages/workflow/common/updown/uploadserror.jsp" />
		</action>
		<action path="/fileDownload" name="fileDownloadForm"
			type="com.neusoft.uniflow.web.common.updown.actions.FileDownloadAction"
			validate="true" scope="request">
			<forward name="failure"
				path="/unieap/pages/workflow/common/updown/downloaderror.jsp" />
			<forward name="success"
				path="/unieap/pages/workflow/common/updown/success.jsp" />
		</action>
		<!-- add by hushiyuan 2008.03.07 workday category list tree-->
		<action path="/workdayCategory" name="workdaycategoryListForm" 
			type="com.neusoft.uniflow.web.common.workdayorgtree.actions.WorkDayOrgTreeAction" scope="request">
			<forward name="sucess" path="/unieap/pages/workflow/common/workdaycatetree/workdaycategorytree.jsp"/>
		</action>
		<action path="/scheduleAction" forward="/unieap/pages/workflow/management/workdaymgt/schedultIndex.jsp"></action>
		<action path="/procauthtree" name="oneActorSelForm" type="com.neusoft.uniflow.web.common.trees.actions.ProcAuthAction" scope="request">
			<forward name="success" path="/unieap/pages/workflow/common/procauthtree.jsp"></forward>
		</action>
		<!-- end add by hushiyuan 2008.03.07 -->
		<action path="/Main" forward="/unieap/pages/workflow/common/Main.jsp"/>
	</action-mappings>
	<!-- Message Resources Definitions -->
	<message-resources key="uniflow" parameter="unieap_workflow" />
</struts-config>
