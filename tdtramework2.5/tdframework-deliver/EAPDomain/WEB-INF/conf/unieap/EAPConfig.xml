<?xml version="1.0" encoding="UTF-8"?>
<eapConfig>

	<component name="cache" isEnabled="true" handler="com.neusoft.unieap.config.handler.CacheHandler" description="">
	    <CONFIG_BEAN>CacheManagerImpl</CONFIG_BEAN>
	    <GROUP_NAME>cacheSynchronize</GROUP_NAME>
	    <CONFIG_BEAN_FILE>/WEB-INF/conf/unieap/cache/applicationContext-cache.xml</CONFIG_BEAN_FILE>
	    <CONFIG_File>WEB-INF/conf/unieap/cache/ehcache.xml</CONFIG_File>
	</component>
	
	<component name="hibernate" isEnabled="false" handler="com.neusoft.unieap.config.handler.HibernateHandler" description="">
	</component>
	
    <component name="static config" isEnabled="true" handler="com.neusoft.unieap.config.handler.SystemConfigHandler"  description="">     
        <WEB_APP_NAME>crm4</WEB_APP_NAME>	      
		<REQUEST_DECODE>true</REQUEST_DECODE>	
		<!-- DB_DEFAULT_DATE_FORMAT is the default format when the date column display in datawindow -->
		<!-- valid value:  YYYY|YYYYMM|YYYYMMDD|YYYY-MM|YYYY-MM-DD|YYYYMMDD HH:MI:SS|YYYY-MM-DD HH:MI:SS|YYYYMMDD HH:MM:SS|YYYY-MM-DD HH:MM:SS -->
		<DB_DEFAULT_DATE_FORMAT>YYYY-MM-DD</DB_DEFAULT_DATE_FORMAT>
		<!-- valid value: #,###.00 | #,###.##-->
		<DB_DEFAULT_MONEY_FORMAT>#,###.00</DB_DEFAULT_MONEY_FORMAT>
		<!-- get the path of big object stored  -->
		<DEFAULT_INTERACTION_OBJECT_FACTORY>com.neusoft.unieap.interaction.DefaultAppLogicObjectFactory</DEFAULT_INTERACTION_OBJECT_FACTORY>
		<DEFAULT_APPLOGIC_OBJECT_FACTORY>com.neusoft.unieap.applogic.AppLogicObjectFactory</DEFAULT_APPLOGIC_OBJECT_FACTORY>
	</component>	
	
	<component name="udolink" isEnabled="false" handler="com.neusoft.unieap.config.handler.UDOLinkConfigHandler" description="">
	    <OP_SYS_PATH>WEB-INF/conf/unieap/udolink/</OP_SYS_PATH>
	    <DATASTORE_CONF_PATH>WEB-INF/conf/unieap/dataset</DATASTORE_CONF_PATH>
	</component>

	<component name="uniecs" isEnabled="false" handler="com.neusoft.unieap.config.handler.UniECSConfigHandler" description="">
        <UNIECS_XML>WEB-INF/conf/unieap/uniecs/ExcConfig.xml</UNIECS_XML>
	</component>
		
	<component name="tc" isEnabled="true" handler="com.neusoft.unieap.config.handler.TCConfigHandler" description="">
	        <TC_CONF_FILE>/WEB-INF/conf/unieap/tc/tcConfig.xml</TC_CONF_FILE>
	        <TC_BASE_FILE>/WEB-INF/conf/unieap/tc/BaseConfig.xml</TC_BASE_FILE>
	        <CONTAINER_CONF_FILE>/WEB-INF/conf/unieap/tc/ContainerConfig.properties</CONTAINER_CONF_FILE>			
	</component>
	
	<component name="datawindow" isEnabled="false" handler="com.neusoft.unieap.config.handler.DataWindowConfigHandler" description="">
	        <DATAWINDOW_CONF_PATH>/WEB-INF/conf/unieap/datawindow/</DATAWINDOW_CONF_PATH>
	        <SAVEDATA_IN_SESSION>true</SAVEDATA_IN_SESSION>  <!-- set false to don't save in session;default is true -->
	        <LOADER_CLASS>com.neusoft.unieap.comp.datawindow.metadata.impl.XmlBeanDWMetaDataLoader</LOADER_CLASS>
	        <SPLIT_STRING>.</SPLIT_STRING>
	        <!--
		<set-property value="WEB-INF/conf/datawindow/" property="DATAWINDOW_CONF_PATH"/>
		<set-property value="WEB-INF/conf/dataset/" property="DATASET_CONF_PATH"/>
		<set-property value="WEB-INF/conf/bocontroller/bocontroller.xml" property="BOCONTROLLER_CONF_PATH"/>
		<set-property value="WEB-INF/conf/bidatawindowconf/bidatawindwowconf.xml" property="BIDATAWIDOW_CONF_FILE"/>
		-->
	</component>
	
	<component name="codelist" isEnabled="false" handler="com.neusoft.unieap.config.handler.CodeListConfigHandler" description="">
	    <CODECACHE_CLASS>com.neusoft.unieap.comp.codelist.impl.CodeListCacheAgentImpl</CODECACHE_CLASS>
	</component>
	
	<component name="tabpage" isEnabled="true" handler="com.neusoft.unieap.config.handler.TabpageConfigHandler" description="">
	    <TABPAGE_XMLNS>XMLNS:TabPageXMLNS</TABPAGE_XMLNS>
	    <OTHER_XMLNS></OTHER_XMLNS>
	</component>

	<component name="report" isEnabled="true" handler="com.neusoft.unieap.config.handler.ReportConfigHandler" description="">
	    <REPORT_XML>WEB-INF/conf/unieap/report/xml</REPORT_XML>
	    <REPORT_XLS>WEB-INF/conf/unieap/report/xls</REPORT_XLS>
	</component>
	
	<component name="pdf" isEnabled="false" handler="com.neusoft.unieap.config.handler.PdfConfigHandler" description="">
	    <BILL_XML>WEB-INF/conf/unieap/bill</BILL_XML>
	</component>
	
	<component name="org" isEnabled="false" handler="com.neusoft.unieap.config.handler.OrgConfigHandler" description="">
	    <EXTINFO_FILE>WEB-INF/conf/unieap/org/extinfo.xml</EXTINFO_FILE>
	    <BASICINFO_FILE>WEB-INF/conf/unieap/org/basicinfo.xml</BASICINFO_FILE>
		<!--base on DB-->
	    <ORGFACTORY>com.neusoft.unieap.service.org.db.DBOrgFactory</ORGFACTORY>
	    <!--Grade Policy-->
	    <isGradePolicy>WEB-INF/conf/unieap/org/filter.xml</isGradePolicy>
	    <!--base on LDAP-->
	    <!--
	    <ORGFACTORY>com.neusoft.unieap.service.org.ldap.LDAPOrgFactory</ORGFACTORY>
	    <LDAP_SERVER>neusoft-wanghx.iplanet:389</LDAP_SERVER>
	    <LDAP_CN>CN=Directory Manager</LDAP_CN>
	    <LDAP_PASSWORD>11111111</LDAP_PASSWORD>
	    -->
	</component>
	
	<component name="bl" isEnabled="true" handler="com.neusoft.unieap.config.handler.BLConfigHandler" description="">
		<CONFIG_PATH>/WEB-INF/conf/om/bl</CONFIG_PATH>
		<CONFIG_FILE>applicationContext.xml dataAccessContext-om.xml applicationContext-om.xml applicationContext-admin.xml portalContext.xml</CONFIG_FILE>
		<InteractionObjectFactoryImpl>com.neusoft.unieap.bl.interaction.SpringInteractionObjectFactory</InteractionObjectFactoryImpl>
	</component>
	
	<component name="dataAuth" isEnabled="false" handler="com.neusoft.unieap.config.handler.DataAuthConfigHandler" description="">
		<ROW_ISENABLED>false</ROW_ISENABLED>
		<COL_ISENABLED>false</COL_ISENABLED>
		<EXPRESSION_IDENTIFIER>$</EXPRESSION_IDENTIFIER>
	</component>
	<component name="innertree" isEnabled="true" handler="com.neusoft.unieap.config.handler.InnerTreeConfigHandler" description="">
	    <CONFIG_FILE>/WEB-INF/conf/unieap/innertree/innertree.xml</CONFIG_FILE>
	</component>
	
	<component name="scheduling" isEnabled="false" handler="com.neusoft.unieap.config.handler.SchedulingHandler" description="">
	    <CONFIG_File>/WEB-INF/conf/unieap/scheduling/applicationContext-job.xml</CONFIG_File>
	</component>
	<component name="security" isEnabled="true" handler="com.neusoft.unieap.config.handler.SecurityHandler" description="">
	    <CONFIG_File>/WEB-INF/conf/unieap/security/applicationContext.xml</CONFIG_File>
	</component>
	<component name="accountPolicy" isEnabled="false" handler="com.neusoft.unieap.config.handler.AccountPolicyHandler" description="">
	    <IS_IP_LOCKED>false</IS_IP_LOCKED>
	    <IS_ACCOUNT_LOCKED>false</IS_ACCOUNT_LOCKED>
	    <FAIL_COUNT>2</FAIL_COUNT>
	    <LOCKED_TIME>2</LOCKED_TIME>
	    <IS_LOAD_FROM_DB>true</IS_LOAD_FROM_DB>
	    <INTERVAL_TIME>30000</INTERVAL_TIME>
	    <IS_ACCOUNT_KICKED>false</IS_ACCOUNT_KICKED>
	    <IS_PERSISTENCE_TO_DB>false</IS_PERSISTENCE_TO_DB>
	    <LOCKING_PAGE>/login.do?method=locking</LOCKING_PAGE>
	    <KICKED_PAGE>/login.do?method=kicked</KICKED_PAGE>
	</component>
	<component name="cacheSynchronize" isEnabled="false" handler="com.neusoft.unieap.config.handler.CacheSynchronizeHandler" description="">
	    <start_time>0</start_time>
	    <interval_time>10000</interval_time>
	    <menu>com.neusoft.unieap.comp.cacheSynchronize.impl.MenuCacheUpdate</menu>
	    <org>com.neusoft.unieap.comp.cacheSynchronize.impl.OrgCacheUpdate</org>
	    <datasecurity>com.neusoft.unieap.comp.cacheSynchronize.impl.DataSecurityCacheUpdate</datasecurity>
	</component>
	<component name="userlist" isEnabled="false" handler="com.neusoft.unieap.config.handler.UserListConfigHandler" description="">
	</component>
	
	<component name="ssl" isEnabled="false" handler="com.neusoft.unieap.config.handler.SSLHandler" description="">
	    <HTTP_PORT>8080</HTTP_PORT>
	    <HTTPS_PORT>8443</HTTPS_PORT>
	</component>
   <component name="jcaptcha" isEnabled="true" handler="com.neusoft.unieap.config.handler.JcaptchaHandler" description="">
  <acceptedChars>0123456789</acceptedChars> 
  <minWordLength>4</minWordLength> 
  <maxWordLength>4</maxWordLength> 
  <minFontSize>19</minFontSize> 
  <maxFontSize>19</maxFontSize>   
  <bgWidth>80</bgWidth> 
  <bgHeight>35</bgHeight>   
  <textColor>black</textColor>
  <bgColor>white</bgColor> 
 </component>
</eapConfig>
