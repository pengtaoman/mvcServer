<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="http://com/neusoft/unieap/service/cache">
	<!-- DEFAULT CACHE REGION -->
	<region name="default">
		<!-- sets the default aux value for any non configured caches -->
		<param name="eap.default" value="DC"/>
		<param name="eap.default.cacheattributes" value="org.apache.jcs.engine.CompositeCacheAttributes"/>
		<param name="eap.default.cacheattributes.MaxObjects" value="10000"/>
		<param name="eap.default.cacheattributes.MemoryCacheName" value="org.apache.jcs.engine.memory.lru.LRUMemoryCache"/>
		<param name="eap.default.elementattributes" value="org.apache.jcs.engine.ElementAttributes"/>
		<param name="eap.default.elementattributes.IsEternal" value="true"/>
		<param name="eap.default.elementattributes.IsSpool" value="true"/>
	</region>
	<!-- DEFAULT CACHE REGION -->
	<!-- SYSTEM CACHE -->
	<region name="system">
		<param name="eap.system.groupIdCache" value="DC"/>
		<param name="eap.system.groupIdCache.cacheattributes" value="org.apache.jcs.engine.CompositeCacheAttributes"/>
		<param name="eap.system.groupIdCache.cacheattributes.MaxObjects" value="10000"/>
		<param name="eap.system.groupIdCache.cacheattributes.MemoryCacheName" value="org.apache.jcs.engine.memory.lru.LRUMemoryCache"/>
	</region>
	<!-- SYSTEM CACHE -->
	
	<region name="DC">
		<!-- sets the default aux value for any non configured caches -->
		<param name="eap.auxiliary.DC" value="org.apache.jcs.auxiliary.disk.indexed.IndexedDiskCacheFactory"/>
		<param name="eap.auxiliary.DC.attributes" value="org.apache.jcs.auxiliary.disk.indexed.IndexedDiskCacheAttributes"/>
		<param name="eap.auxiliary.DC.attributes.DiskPath" value="c:/raf"/>
	</region>
	
	<region name="EAPServices">
		<!-- sets the default aux value for any non configured caches -->
		<param name="eap.region.EAPServices" value="DC"/>
		<param name="eap.region.EAPServices.cacheattributes" value="org.apache.jcs.engine.CompositeCacheAttributes"/>
		<param name="eap.region.EAPServices.cacheattributes.MaxObjects" value="100"/>
		<param name="eap.region.EAPServices.cacheattributes.MemoryCacheName" value="org.apache.jcs.engine.memory.lru.LRUMemoryCache"/>
		<param name="eap.region.EAPServices.elementattributes" value="org.apache.jcs.engine.ElementAttributes"/>
		<param name="eap.region.EAPServices.elementattributes.IsEternal" value="false"/>
		<param name="eap.region.EAPServices.elementattributes.IsSpool" value="true"/>
	</region>
	<region name="DataSecurity">
		<!-- sets the default aux value for any non configured caches -->
		<param name="eap.region.DataSecurity.cacheattributes" value="org.apache.jcs.engine.CompositeCacheAttributes"/>
		<param name="eap.region.DataSecurity.cacheattributes.MaxObjects" value="100"/>
		<param name="eap.region.DataSecurity.cacheattributes.MemoryCacheName" value="org.apache.jcs.engine.memory.lru.LRUMemoryCache"/>
		<param name="eap.region.DataSecurity.elementattributes" value="org.apache.jcs.engine.ElementAttributes"/>
		<param name="eap.region.DataSecurity.elementattributes.IsEternal" value="false"/>
	</region>
	<region name="UniSecurity">
		<param name="eap.region.UniSecurity" value=""/>
		<param name="eap.region.UniSecurity.cacheattributes" value="org.apache.jcs.engine.CompositeCacheAttributes"/>
		<param name="eap.region.UniSecurity.cacheattributes.MaxObjects" value="200"/>
		<param name="eap.region.UniSecurity.cacheattributes.MemoryCacheName" value="org.apache.jcs.engine.memory.lru.LRUMemoryCache"/>
		<param name="eap.region.UniSecurity.cacheattributes.UseMemoryShrinker" value="true"/>
		<param name="eap.region.UniSecurity.cacheattributes.ShrinkerIntervalSeconds" value="60"/>
		<param name="eap.region.UniSecurity.elementattributes" value="org.apache.jcs.engine.ElementAttributes"/>
		<param name="eap.region.UniSecurity.elementattributes.IsEternal" value="false"/>
		<param name="eap.region.UniSecurity.elementattributes.MaxLifeSeconds" value="480"/>
		<param name="eap.region.UniSecurity.elementattributes.IdleTime" value="240"/>
		<param name="eap.region.UniSecurity.elementattributes.IsSpool" value="false"/>
	</region>
</configuration>
